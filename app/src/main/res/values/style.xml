<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="container_top">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">260dp</item>
        <item name="android:background">@drawable/baground_top</item>
    </style>

    <style name="container_top_pesquisar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">260dp</item>
        <item name="android:background">@drawable/background_pesquisar</item>
    </style>


    <style name="container_labels">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/label_layout</item>
        <item name="android:padding">13dp</item>
        <item name="android:layout_marginLeft">18dp</item>
        <item name="android:layout_marginRight">18dp</item>
    </style>

    <style name="container_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/transparente</item>
        <item name="android:layout_marginLeft">18dp</item>
        <item name="android:layout_marginRight">18dp</item>
    </style>

    <style name="container_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/branco</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:padding">13dp</item>
        <item name="android:layout_marginLeft">18dp</item>
        <item name="android:layout_marginRight">18dp</item>
    </style>

    <style name="container_images" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">15dp</item>
        <item name="color">@color/branco</item>

    </style>
    
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/color_primary</item>
        <item name="android:background">@drawable/label_layout</item>
        <item name="android:padding">13dp</item>
        <item name="android:layout_marginLeft">18dp</item>
        <item name="android:layout_marginRight">18dp</item>
    </style>

    <style name="container_down">
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/container_baixo</item>
    </style>

    <style name="container_conteudo">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">460dp</item>
        <item name="android:background">@drawable/backgruondfundo</item>
    </style>









</resources>